{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "NLog": {
      "ConfigPath": "nlog.config"
    }
  },
  "MinIO": {
    "Endpoint": "minio:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "files",
    "TmpFolder": "tmp",
    "PublicFolder": "videos"
  },
  "MessageBroker": {
    "Host": "rabbitmq",
    "Username": "rabbitmq",
    "Password": "rabbitmq",
    "VideoProcessingExchangeName" : "video-processing-request"
  },
  "Conversion": {
    "SegmentDurationInSeconds": 10,
    "AddLetterbox": true,
    "Resolutions": [
      {
        "Width": 1280,
        "Height": 720,
        "Bitrate": "5000k"
      },
      {
        "Width": 854,
        "Height": 480,
        "Bitrate": "3000k"
      }
    ]
  }
}
